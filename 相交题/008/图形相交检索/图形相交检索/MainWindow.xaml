<Window x:Class="Geone.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="图形相交检索" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="80"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.5*"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <ProgressBar Name="pb" Grid.Row="1" Grid.Column="0" Maximum="1" Minimum="0" Margin="2,10"></ProgressBar>


        <Button Name="btn" Grid.Row="1" Grid.Column="1"  Click="Button_Click" Margin="2">分析</Button>


        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Name="txtTime" Grid.Row="2" Grid.Column="0" ></TextBlock>
    </Grid>
</Window>
